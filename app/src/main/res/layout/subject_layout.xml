<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="@dimen/gutter"
    android:layout_marginEnd="@dimen/gutter"
    android:layout_marginBottom="@dimen/margin8dp"
    app:cardElevation="4dp">

    <TextView
        android:id="@+id/tv_mata_kuliah"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_weight="1"
        android:padding="@dimen/gutter"
        android:textColor="@color/black"
        app:drawableEndCompat="@drawable/ic_next"
        tools:drawableEnd="@drawable/ic_next"
        tools:text="@string/subject_placeholder" />
</androidx.cardview.widget.CardView>